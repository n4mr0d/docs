---
import '../styles/global.css';
const {
  title = 'DProdz | Algerian Digital Innovators',
  description = 'DProdz propels Algerian businesses with futuristic SEO, cybersecurity, design, web development, hosting, and IT consultancy.'
} = Astro.props;
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
    <meta name="description" content={description} />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&display=swap"
      rel="stylesheet"
    />
    <link rel="icon" type="image/svg+xml" href="https://dprodz.com/wp-content/uploads/2024/11/dpro-Logo-04.svg" />
    <meta name="theme-color" content="#050b16" />
  </head>
  <body>
    <slot />
  </body>
</html>
